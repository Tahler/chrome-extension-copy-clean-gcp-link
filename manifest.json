{
  "manifest_version": 3,
  "name": "Copy clean Google Cloud Console link",
  "version": "1.0",
  "description": "A Chrome extension to copy Google Cloud Console links but without common and noisy query parameters.",
  "permissions": ["activeTab", "scripting", "clipboardWrite"],
  "background": {
    "service_worker": "main.js"
  },
  "commands": {
    "copy-link": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      },
      "description": "Copies link"
    }
  },
  "action": {
    "default_title": "Google Cloud Console Link Copier"
  }
}
